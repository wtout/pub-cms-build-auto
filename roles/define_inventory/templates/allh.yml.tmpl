      disk3: "{{ environment_definition.vm_setting.em7.em7dc.disk3 if (environment_definition.vm_setting.em7.em7dc.disk3 is defined and environment_definition.vm_setting.em7.em7dc.disk3 != none) else '0' }}"

      vmname: "{{ environment_definition.vm_setting.em7.em7dc.vmname if (environment_definition.vm_setting.em7.em7dc.vmname is defined and environment_definition.vm_setting.em7.em7dc.vmname != none) else '' }}"

      # Network interface 1
      ################
      nic1:

        # Interface name
        intname: "{{ environment_definition.vm_setting.em7.em7dc.nic1.intname if (environment_definition.vm_setting.em7.em7dc.nic1.intname is defined and environment_definition.vm_setting.em7.em7dc.nic1.intname != none) else 'ens160' }}"

