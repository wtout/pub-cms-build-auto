        # IP fourth octet pool
        ip_octetpool: "{{ environment_definition.vm_setting.em7.em7dc.nic1.ip_octetpool if (environment_definition.vm_setting.em7.em7dc.nic1.ip_octetpool is defined and environment_definition.vm_setting.em7.em7dc.nic1.ip_octetpool != none) else [ '9', '11', '10', '12', '15', '17', '16', '18', '26', '27', '28', '29' ] }}"

        # Gateway
        gtw: "{{ environment_definition.vm_setting.em7.em7dc.nic1.gtw if (environment_definition.vm_setting.em7.em7dc.nic1.gtw is defined and environment_definition.vm_setting.em7.em7dc.nic1.gtw != none) else '' }}"

        # Gateway fourth octet
        gtw_octet: "{{ environment_definition.vm_setting.em7.em7dc.nic1.gtw_octet if (environment_definition.vm_setting.em7.em7dc.nic1.gtw_octet is defined and environment_definition.vm_setting.em7.em7dc.nic1.gtw_octet != none) else '' }}"

        # Netmask
        netmask: "{{ environment_definition.vm_setting.em7.em7dc.nic1.netmask if (environment_definition.vm_setting.em7.em7dc.nic1.netmask is defined and environment_definition.vm_setting.em7.em7dc.nic1.netmask != none) else '255.255.255.192' }}"

        # Net Name
        netname: "{{ environment_definition.vm_setting.em7.em7dc.nic1.netname if (environment_definition.vm_setting.em7.em7dc.nic1.netname is defined and environment_definition.vm_setting.em7.em7dc.nic1.netname != none) else '' }}"

        # Net Base
        netbase: '{{ environment_definition.vm_setting.em7.em7dc.nic1.network if (environment_definition.vm_setting.em7.em7dc.nic1.network is defined and environment_definition.vm_setting.em7.em7dc.nic1.network != none) else "-0-26-int" }}'

      # Network interface 2
      ################
      nic2:

        # Interface name
        intname: "{{ environment_definition.vm_setting.em7.em7dc.nic2.intname if (environment_definition.vm_setting.em7.em7dc.nic2.intname is defined and environment_definition.vm_setting.em7.em7dc.nic2.intname != none) else 'ens192' }}"

        # IP pool
        ip_pool: "{{ environment_definition.vm_setting.em7.em7dc.nic2.ip_pool if (environment_definition.vm_setting.em7.em7dc.nic2.ip_pool is defined and environment_definition.vm_setting.em7.em7dc.nic2.ip_pool != none) else [] }}"

        # IP fourth octet pool
        ip_octetpool: "{{ environment_definition.vm_setting.em7.em7dc.nic2.ip_octetpool if (environment_definition.vm_setting.em7.em7dc.nic2.ip_octetpool is defined and environment_definition.vm_setting.em7.em7dc.nic2.ip_octetpool != none) else [ '65', '66', '67', '68', '69', '70', '71', '72', '86', '87', '88', '89' ] }}"

        # Gateway
        gtw: "{{ environment_definition.vm_setting.em7.em7dc.nic2.gtw if (environment_definition.vm_setting.em7.em7dc.nic2.gtw is defined and environment_definition.vm_setting.em7.em7dc.nic2.gtw != none) else '' }}"

        # Gateway fourth octet
        gtw_octet: "{{ environment_definition.vm_setting.em7.em7dc.nic2.gtw_octet if (environment_definition.vm_setting.em7.em7dc.nic2.gtw_octet is defined and environment_definition.vm_setting.em7.em7dc.nic2.gtw_octet != none) else '94' }}"

        # Netmask
        netmask: "{{ environment_definition.vm_setting.em7.em7dc.nic2.netmask if (environment_definition.vm_setting.em7.em7dc.nic2.netmask is defined and environment_definition.vm_setting.em7.em7dc.nic2.netmask != none) else '255.255.255.224' }}"

        # Net Name
        netname: "{{ environment_definition.vm_setting.em7.em7dc.nic2.netname if (environment_definition.vm_setting.em7.em7dc.nic2.netname is defined and environment_definition.vm_setting.em7.em7dc.nic2.netname != none) else '' }}"

        # Net Base
        netbase: '{{ environment_definition.vm_setting.em7.em7dc.nic2.network if (environment_definition.vm_setting.em7.em7dc.nic2.network is defined and environment_definition.vm_setting.em7.em7dc.nic2.network != none) else "-64-27-cust" }}'


    ################
    # EM7 Message collectors
    ################
    em7mc:

{%- if environment_definition.vm_setting.em7.em7mc.user.name is defined and environment_definition.vm_setting.em7.em7mc.user.name != none and environment_definition.vm_setting.em7.em7mc.user.name != '' and environment_definition.vm_setting.em7.em7mc.user.password is defined and environment_definition.vm_setting.em7.em7mc.user.password != none and environment_definition.vm_setting.em7.em7mc.user.password != '' %}
{% block em7mcusers %}
      # User credentials
      user:
        name: "{{ environment_definition.vm_setting.em7.em7mc.user.name }}"
        password: "{{ environment_definition.vm_setting.em7.em7mc.user.password }}"
{%- endblock em7mcusers %}
{%- endif %}

      # number of CPUs
