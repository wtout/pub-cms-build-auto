      disk2: "{{ environment_definition.vm_setting.em7.em7prt.disk2 if (environment_definition.vm_setting.em7.em7prt.disk2 is defined and environment_definition.vm_setting.em7.em7prt.disk2 != none) else '0' }}"
      disk3: "{{ environment_definition.vm_setting.em7.em7prt.disk3 if (environment_definition.vm_setting.em7.em7prt.disk3 is defined and environment_definition.vm_setting.em7.em7prt.disk3 != none) else '0' }}"

      vmname: "{{ environment_definition.vm_setting.em7.em7prt.vmname if (environment_definition.vm_setting.em7.em7prt.vmname is defined and environment_definition.vm_setting.em7.em7prt.vmname != none) else '' }}"

      # Network interface 1
      ################
      nic1:

        # Interface name
        intname: "{{ environment_definition.vm_setting.em7.em7prt.nic1.intname if (environment_definition.vm_setting.em7.em7prt.nic1.intname is defined and environment_definition.vm_setting.em7.em7prt.nic1.intname != none) else 'ens160' }}"

