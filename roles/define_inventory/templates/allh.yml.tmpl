      # Disks sizing in GiB
      disk0: "{{ environment_definition.vm_setting.em7.em7mc.disk0 if (environment_definition.vm_setting.em7.em7mc.disk0 is defined and environment_definition.vm_setting.em7.em7mc.disk0 != none) else '90' }}"
      disk1: "{{ environment_definition.vm_setting.em7.em7mc.disk1 if (environment_definition.vm_setting.em7.em7mc.disk1 is defined and environment_definition.vm_setting.em7.em7mc.disk1 != none) else '0' }}"
      disk2: "{{ environment_definition.vm_setting.em7.em7mc.disk2 if (environment_definition.vm_setting.em7.em7mc.disk2 is defined and environment_definition.vm_setting.em7.em7mc.disk2 != none) else '0' }}"
      disk3: "{{ environment_definition.vm_setting.em7.em7mc.disk3 if (environment_definition.vm_setting.em7.em7mc.disk3 is defined and environment_definition.vm_setting.em7.em7mc.disk3 != none) else '0' }}"

      vmname: "{{ environment_definition.vm_setting.em7.em7mc.vmname if (environment_definition.vm_setting.em7.em7mc.vmname is defined and environment_definition.vm_setting.em7.em7mc.vmname != none) else '' }}"

      # Network interface 1
      ################
      nic1:

        # Interface name
        intname: "{{ environment_definition.vm_setting.em7.em7mc.nic1.intname if (environment_definition.vm_setting.em7.em7mc.nic1.intname is defined and environment_definition.vm_setting.em7.em7mc.nic1.intname != none) else 'ens160' }}"

        # IP pool
        ip_pool: "{{ environment_definition.vm_setting.em7.em7mc.nic1.ip_pool if (environment_definition.vm_setting.em7.em7mc.nic1.ip_pool is defined and environment_definition.vm_setting.em7.em7mc.nic1.ip_pool != none) else [] }}"

        # IP fourth octet pool
        ip_octetpool: "{{ environment_definition.vm_setting.em7.em7mc.nic1.ip_octetpool if (environment_definition.vm_setting.em7.em7mc.nic1.ip_octetpool is defined and environment_definition.vm_setting.em7.em7mc.nic1.ip_octetpool != none) else [ '5', '7', '30', '31', '32', '33' ] }}"

        # Gateway
        gtw: "{{ environment_definition.vm_setting.em7.em7mc.nic1.gtw if (environment_definition.vm_setting.em7.em7mc.nic1.gtw is defined and environment_definition.vm_setting.em7.em7mc.nic1.gtw != none) else '' }}"

        # Gateway fourth octet
        gtw_octet: "{{ environment_definition.vm_setting.em7.em7mc.nic1.gtw_octet if (environment_definition.vm_setting.em7.em7mc.nic1.gtw_octet is defined and environment_definition.vm_setting.em7.em7mc.nic1.gtw_octet != none) else '62' }}"

        # Netmask
        netmask: "{{ environment_definition.vm_setting.em7.em7mc.nic1.netmask if (environment_definition.vm_setting.em7.em7mc.nic1.netmask is defined and environment_definition.vm_setting.em7.em7mc.nic1.netmask != none) else '255.255.255.192' }}"

        # Net Name
        netname: "{{ environment_definition.vm_setting.em7.em7mc.nic1.netname if (environment_definition.vm_setting.em7.em7mc.nic1.netname is defined and environment_definition.vm_setting.em7.em7mc.nic1.netname != none) else '' }}"

        # Net Base
        netbase: '{{ environment_definition.vm_setting.em7.em7mc.nic1.network if (environment_definition.vm_setting.em7.em7mc.nic1.network is defined and environment_definition.vm_setting.em7.em7mc.nic1.network != none) else "-0-26-int" }}'

      # Network interface 2
      ################
      nic2:

        # Interface name
        intname: "{{ environment_definition.vm_setting.em7.em7mc.nic2.intname if (environment_definition.vm_setting.em7.em7mc.nic2.intname is defined and environment_definition.vm_setting.em7.em7mc.nic2.intname != none) else '' }}"

        # IP pool
        ip_pool: "{{ environment_definition.vm_setting.em7.em7mc.nic2.ip_pool if (environment_definition.vm_setting.em7.em7mc.nic2.ip_pool is defined and environment_definition.vm_setting.em7.em7mc.nic2.ip_pool != none) else [] }}"

        # IP fourth octet pool
        ip_octetpool: "{{ environment_definition.vm_setting.em7.em7mc.nic2.ip_octetpool if (environment_definition.vm_setting.em7.em7mc.nic2.ip_octetpool is defined and environment_definition.vm_setting.em7.em7mc.nic2.ip_octetpool != none) else [] }}"

        # Gateway
        gtw: "{{ environment_definition.vm_setting.em7.em7mc.nic2.gtw if (environment_definition.vm_setting.em7.em7mc.nic2.gtw is defined and environment_definition.vm_setting.em7.em7mc.nic2.gtw != none) else '' }}"

        # Gateway fourth octet
        gtw_octet: "{{ environment_definition.vm_setting.em7.em7mc.nic2.gtw_octet if (environment_definition.vm_setting.em7.em7mc.nic2.gtw_octet is defined and environment_definition.vm_setting.em7.em7mc.nic2.gtw_octet != none) else '' }}"

        # Netmask
        netmask: "{{ environment_definition.vm_setting.em7.em7mc.nic2.netmask if (environment_definition.vm_setting.em7.em7mc.nic2.netmask is defined and environment_definition.vm_setting.em7.em7mc.nic2.netmask != none) else '' }}"

        # Net Name
        netname: "{{ environment_definition.vm_setting.em7.em7mc.nic2.netname if (environment_definition.vm_setting.em7.em7mc.nic2.netname is defined and environment_definition.vm_setting.em7.em7mc.nic2.netname != none) else '' }}"

        # Net Base
        netbase: "{{ environment_definition.vm_setting.em7.em7mc.nic2.network if (environment_definition.vm_setting.em7.em7mc.nic2.network is defined and environment_definition.vm_setting.em7.em7mc.nic2.network != none) else '' }}"


    ################
    # EM7 portals
    ################
    em7prt:

{%- if environment_definition.vm_setting.em7.em7prt.user.name is defined and environment_definition.vm_setting.em7.em7prt.user.name != none and environment_definition.vm_setting.em7.em7prt.user.name != '' and environment_definition.vm_setting.em7.em7prt.user.password is defined and environment_definition.vm_setting.em7.em7prt.user.password != none and environment_definition.vm_setting.em7.em7prt.user.password != '' %}
