##################################################################################
# VM specific information
##################################################################################
vm_vars:

  ################
  # EM7 VMs
  ################
  em7:

    ################
    # EM7 Databases
    ################
    em7db:

{%- if environment_definition.vm_setting.em7.em7db.user.name is defined and environment_definition.vm_setting.em7.em7db.user.name != none and environment_definition.vm_setting.em7.em7db.user.name != '' and environment_definition.vm_setting.em7.em7db.user.password is defined and environment_definition.vm_setting.em7.em7db.user.password != none and environment_definition.vm_setting.em7.em7db.user.password != '' %}
{% block em7dbusers %}
      # User credentials
      user:
        name: "{{ environment_definition.vm_setting.em7.em7db.user.name }}"
        password: "{{ environment_definition.vm_setting.em7.em7db.user.password }}"
{%- endblock em7dbusers %}
{%- endif %}

      # number of CPUs
      cpu: "{{ environment_definition.vm_setting.em7.em7db.cpu if (environment_definition.vm_setting.em7.em7db.cpu is defined and environment_definition.vm_setting.em7.em7db.cpu != none) else '12' }}"

      # Memory size in GiB
      memory: "{{ environment_definition.vm_setting.em7.em7db.memory if (environment_definition.vm_setting.em7.em7db.memory is defined and environment_definition.vm_setting.em7.em7db.memory != none) else '256' }}"

      # Disks sizing in GiB
      disk0: "{{ environment_definition.vm_setting.em7.em7db.disk0 if (environment_definition.vm_setting.em7.em7db.disk0 is defined and environment_definition.vm_setting.em7.em7db.disk0 != none) else '1024' }}"
      disk1: "{{ environment_definition.vm_setting.em7.em7db.disk1 if (environment_definition.vm_setting.em7.em7db.disk1 is defined and environment_definition.vm_setting.em7.em7db.disk1 != none) else '0' }}"
      disk2: "{{ environment_definition.vm_setting.em7.em7db.disk2 if (environment_definition.vm_setting.em7.em7db.disk2 is defined and environment_definition.vm_setting.em7.em7db.disk2 != none) else '0' }}"
      disk3: "{{ environment_definition.vm_setting.em7.em7db.disk3 if (environment_definition.vm_setting.em7.em7db.disk3 is defined and environment_definition.vm_setting.em7.em7db.disk3 != none) else '0' }}"

      vmname: "{{ environment_definition.vm_setting.em7.em7db.vmname if (environment_definition.vm_setting.em7.em7db.vmname is defined and environment_definition.vm_setting.em7.em7db.vmname != none) else '' }}"

      # Network interface 1
      ################
      nic1:

        # Interface name
        intname: "{{ environment_definition.vm_setting.em7.em7db.nic1.intname if (environment_definition.vm_setting.em7.em7db.nic1.intname is defined and environment_definition.vm_setting.em7.em7db.nic1.intname != none) else 'ens160' }}"

        # IP pool
        ip_pool: "{{ environment_definition.vm_setting.em7.em7db.nic1.ip_pool if (environment_definition.vm_setting.em7.em7db.nic1.ip_pool is defined and environment_definition.vm_setting.em7.em7db.nic1.ip_pool != none) else [] }}"

        # IP fourth octet pool
        ip_octetpool: "{{ environment_definition.vm_setting.em7.em7db.nic1.ip_octetpool if (environment_definition.vm_setting.em7.em7db.nic1.ip_octetpool is defined and environment_definition.vm_setting.em7.em7db.nic1.ip_octetpool != none) else range(13, 14 + 1)|list }}"

        # Gateway
        gtw: "{{ environment_definition.vm_setting.em7.em7db.nic1.gtw if (environment_definition.vm_setting.em7.em7db.nic1.gtw is defined and environment_definition.vm_setting.em7.em7db.nic1.gtw != none) else '' }}"

        # Gateway fourth octet
        gtw_octet: "{{ environment_definition.vm_setting.em7.em7db.nic1.gtw_octet if (environment_definition.vm_setting.em7.em7db.nic1.gtw_octet is defined and environment_definition.vm_setting.em7.em7db.nic1.gtw_octet != none) else '62' }}"

        # Netmask
        netmask: "{{ environment_definition.vm_setting.em7.em7db.nic1.netmask if (environment_definition.vm_setting.em7.em7db.nic1.netmask is defined and environment_definition.vm_setting.em7.em7db.nic1.netmask != none) else '255.255.255.192' }}"

        # Net Name
        netname: "{{ environment_definition.vm_setting.em7.em7db.nic1.netname if (environment_definition.vm_setting.em7.em7db.nic1.netname is defined and environment_definition.vm_setting.em7.em7db.nic1.netname != none) else '' }}"

        # Net Base
        netbase: '{{ environment_definition.vm_setting.em7.em7db.nic1.network if (environment_definition.vm_setting.em7.em7db.nic1.network is defined and environment_definition.vm_setting.em7.em7db.nic1.network != none) else "-0-26-int" }}'

      # Network interface 2
      ################
      nic2:

        # Interface name
        intname: "{{ environment_definition.vm_setting.em7.em7db.nic2.intname if (environment_definition.vm_setting.em7.em7db.nic2.intname is defined and environment_definition.vm_setting.em7.em7db.nic2.intname != none) else 'ens192' }}"

        # IP pool
        ip_pool: "{{ environment_definition.vm_setting.em7.em7db.nic2.ip_pool if (environment_definition.vm_setting.em7.em7db.nic2.ip_pool is defined and environment_definition.vm_setting.em7.em7db.nic2.ip_pool != none) else [ '192.170.170.9', '192.170.170.10' ] }}"

        # IP fourth octet pool
        ip_octetpool: "{{ environment_definition.vm_setting.em7.em7db.nic2.ip_octetpool if (environment_definition.vm_setting.em7.em7db.nic2.ip_octetpool is defined and environment_definition.vm_setting.em7.em7db.nic2.ip_octetpool != none) else [] }}"

        # Gateway
        gtw: "{{ environment_definition.vm_setting.em7.em7db.nic2.gtw if (environment_definition.vm_setting.em7.em7db.nic2.gtw is defined and environment_definition.vm_setting.em7.em7db.nic2.gtw != none) else '' }}"

        # Gateway fourth octet
        gtw_octet: "{{ environment_definition.vm_setting.em7.em7db.nic2.gtw_octet if (environment_definition.vm_setting.em7.em7db.nic2.gtw_octet is defined and environment_definition.vm_setting.em7.em7db.nic2.gtw_octet != none) else '' }}"

        # Netmask
        netmask: "{{ environment_definition.vm_setting.em7.em7db.nic2.netmask if (environment_definition.vm_setting.em7.em7db.nic2.netmask is defined and environment_definition.vm_setting.em7.em7db.nic2.netmask != none) else '255.255.255.252' }}"

        # Net Name
        netname: "{{ environment_definition.vm_setting.em7.em7db.nic2.netname if (environment_definition.vm_setting.em7.em7db.nic2.netname is defined and environment_definition.vm_setting.em7.em7db.nic2.netname != none) else '' }}"

        # Net Base
        netbase: '{{ environment_definition.vm_setting.em7.em7db.nic2.network if (environment_definition.vm_setting.em7.em7db.nic2.network is defined and environment_definition.vm_setting.em7.em7db.nic2.network != none) else "-db-heartbeat" }}'


    ################
    # EM7 Data collectors
    ################
    em7dc:

{%- if environment_definition.vm_setting.em7.em7dc.user.name is defined and environment_definition.vm_setting.em7.em7dc.user.name != none and environment_definition.vm_setting.em7.em7dc.user.name != '' and environment_definition.vm_setting.em7.em7dc.user.password is defined and environment_definition.vm_setting.em7.em7dc.user.password != none and environment_definition.vm_setting.em7.em7dc.user.password != '' %}
{% block em7dcusers %}
      # User credentials
      user:
        name: "{{ environment_definition.vm_setting.em7.em7dc.user.name }}"
        password: "{{ environment_definition.vm_setting.em7.em7dc.user.password }}"
{%- endblock em7dcusers %}
{%- endif %}

      # number of CPUs
      cpu: "{{ environment_definition.vm_setting.em7.em7dc.cpu if (environment_definition.vm_setting.em7.em7dc.cpu is defined and environment_definition.vm_setting.em7.em7dc.cpu != none) else '4' }}"

      # Memory size in GiB
      memory: "{{ environment_definition.vm_setting.em7.em7dc.memory if (environment_definition.vm_setting.em7.em7dc.memory is defined and environment_definition.vm_setting.em7.em7dc.memory != none) else '24' }}"

      # Disks sizing in GiB
      disk0: "{{ environment_definition.vm_setting.em7.em7dc.disk0 if (environment_definition.vm_setting.em7.em7dc.disk0 is defined and environment_definition.vm_setting.em7.em7dc.disk0 != none) else '150' }}"
      disk1: "{{ environment_definition.vm_setting.em7.em7dc.disk1 if (environment_definition.vm_setting.em7.em7dc.disk1 is defined and environment_definition.vm_setting.em7.em7dc.disk1 != none) else '0' }}"
