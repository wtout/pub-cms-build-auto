      disk3: "{{ environment_definition.vm_setting.em7.em7db.disk3 if (environment_definition.vm_setting.em7.em7db.disk3 is defined and environment_definition.vm_setting.em7.em7db.disk3 != none) else '0' }}"

      name: "{{ environment_definition.vm_setting.em7.em7db.name if (environment_definition.vm_setting.em7.em7db.name is defined and environment_definition.vm_setting.em7.em7db.name != none) else '' }}"

      # Network interface 1
      ################
      nic1:

        # Interface name
        name: "{{ environment_definition.vm_setting.em7.em7db.nic1.name if (environment_definition.vm_setting.em7.em7db.nic1.name is defined and environment_definition.vm_setting.em7.em7db.nic1.name != none) else 'ens160' }}"

        # IP pool
        ip_pool: "{{ environment_definition.vm_setting.em7.em7db.nic1.ip_pool if (environment_definition.vm_setting.em7.em7db.nic1.ip_pool is defined and environment_definition.vm_setting.em7.em7db.nic1.ip_pool != none) else [] }}"

        # IP fourth octet pool
        ip_octetpool: "{{ environment_definition.vm_setting.em7.em7db.nic1.ip_octetpool if (environment_definition.vm_setting.em7.em7db.nic1.ip_octetpool is defined and environment_definition.vm_setting.em7.em7db.nic1.ip_octetpool != none) else range(13, 14 + 1)|list }}"

        # Gateway
        gtw: "{{ environment_definition.vm_setting.em7.em7db.nic1.gtw if (environment_definition.vm_setting.em7.em7db.nic1.gtw is defined and environment_definition.vm_setting.em7.em7db.nic1.gtw != none) else '' }}"

        # Gateway fourth octet
        gtw_octet: "{{ environment_definition.vm_setting.em7.em7db.nic1.gtw_octet if (environment_definition.vm_setting.em7.em7db.nic1.gtw_octet is defined and environment_definition.vm_setting.em7.em7db.nic1.gtw_octet != none) else '62' }}"

        # Netmask
        netmask: "{{ environment_definition.vm_setting.em7.em7db.nic1.netmask if (environment_definition.vm_setting.em7.em7db.nic1.netmask is defined and environment_definition.vm_setting.em7.em7db.nic1.netmask != none) else '255.255.255.192' }}"

