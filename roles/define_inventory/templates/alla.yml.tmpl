##################################################################################
# VM specific information
##################################################################################
vm_vars:

  ################
  # EM7 VMs
  ################
  em7:

    ################
    # EM7 Databases
    ################
    em7db:

      # number of CPUs
      cpu: "{{ environment_definition.vm_setting.em7.em7db.cpu if (environment_definition.vm_setting.em7.em7db.cpu is defined and environment_definition.vm_setting.em7.em7db.cpu != none) else '12' }}"

      # Memory size in GiB
      memory: "{{ environment_definition.vm_setting.em7.em7db.memory if (environment_definition.vm_setting.em7.em7db.memory is defined and environment_definition.vm_setting.em7.em7db.memory != none) else '48' if environment_definition.customer.primary.name_prefix is regex('[\\w]c[\\d]{2,}') else '256' }}"

      # Disks sizing in GiB
      disk0: "{{ environment_definition.vm_setting.em7.em7db.disk0 if (environment_definition.vm_setting.em7.em7db.disk0 is defined and environment_definition.vm_setting.em7.em7db.disk0 != none) else '250' if environment_definition.customer.primary.name_prefix is regex('[\\w]c[\\d]{2,}') else '1024' }}"
